@layout("/common/_container.html"){
<div class="row">
    <div class="col-sm-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>入驻企业管理</h5>
            </div>
            <div class="ibox-content">
                <div class="row row-lg">
                    <div class="col-sm-12">
                        <div class="row">
                        	<div class="col-lg-2 col-sm-3">
								<div class="panel panel-default">
									<div class="panel-heading">组织机构</div>
									<div class="panel-body dept-tree">
										<ul id="catatree" class="ztree"></ul>
									</div>
								</div>
							</div>
                        	<div class="col-lg-10 col-sm-9">
                        	<div class="row">
	                           <div class="col-lg-4 col-sm-12">
	                               <#NameCon id="cpn_cata_nm" name="名称" />
	                           </div>
	                           <div class="col-sm-3">
	                               <#button name="搜索" icon="fa-search" clickFun="Xyd_cpn_cata.search()"/>
	                           </div>
                        	</div>
                        <div class="hidden-xs" id="Xyd_cpn_cataTableToolbar" role="group">
                            @if(shiro.hasPermission("/xyd_cpn_cata/add")){
                                <#button name="添加" icon="fa-plus" clickFun="Xyd_cpn_cata.openAddXyd_cpn_cata()"/>
                            @}
                            @if(shiro.hasPermission("/xyd_cpn_cata/update")){
                                <#button name="修改" icon="fa-edit" clickFun="Xyd_cpn_cata.openXyd_cpn_cataDetail_update()" space="true"/>
                            @}
                            @if(shiro.hasPermission("/xyd_cpn_cata/delete")){
                                <#button name="删除" icon="fa-remove" clickFun="Xyd_cpn_cata.delete()" space="true"/>
                            @}
                            @if(shiro.hasPermission("/xyd_cpn_cata/export")){
                                <#button name="导出" icon="fa-file-excel-o" clickFun="Xyd_cpn_cata.export()" space="true"/>
                            @}
                             @if(shiro.hasPermission("/xyd_cpn_cata/detail")){
                                <#button name="详情" icon="fa-file-code-o" clickFun="Xyd_cpn_cata.openXyd_cpn_cataDetail()" space="true"/>
                            @}
                        </div>
                        <#table id="Xyd_cpn_cataTable"/>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="${ctxPath}/static/addons/zcgl/cpncata/js/xyd_cpn_cata.js"></script>
@}
