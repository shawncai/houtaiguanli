@layout("/common/_container.html"){
<div class="ibox float-e-margins">
    <div class="ibox-content">
        <form class="form-horizontal" id="signupForm">
			<input type="hidden" id="chnl_id" value="">
			   <div class="row">
                                   <#inputCol1 id='chnl_no' name='渠道编号' />
                                   <#inputCol1 id='chnl_nm' name='渠道名称' />
                                   <#inputCol1 id='chnl_desc' name='渠道描述' />
                                   <#inputCol1 id='chnl_phone' name='渠道联系方式' />
                                   <#inputCol1 id='chnl_user_nm' name='渠道用户名称' />
                                   
                                   
                                   
                                   
               </div>
            <div class="row btn-group-m-t text-center">
                <div class="col-sm-10">
                    <#submit btnCss="info" iName="提交" id="ensure" icon="fa-check" clickFun="validate();Bs_sale_channelInfoDlg.addSubmit()"/>
                    <#button btnCss="danger" name="取消" id="cancel" icon="fa-eraser" clickFun="Bs_sale_channelInfoDlg.close()"/>
                </div>
            </div>
         </form>
    </div>
</div>
<script src="${ctxPath}/static/addons/zsh/bs_sale_channel/js/bs_sale_channel_info.js"></script>
@}
