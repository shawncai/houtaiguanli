@layout("/common/_container.html"){
<div class="ibox float-e-margins">
	<div class="ibox-content">
		<div class="form-horizontal">
			<input type="hidden" id="topic_id" value="">
			<table class="table table-striped table-bordered table-hover"
				style="font-size: 16px;">
				<input type="hidden" id="param" value="">
				<tbody id="t_topic">

				</tbody>
			</table>
		</div>
		<div class="row btn-group-m-t">
			<div class="col-sm-6">
				<#button btnCss="info" name="保存" id="ensure" icon="fa-check"
					clickFun="Xgt_wy_topicInfo.add()" />
				<!-- <#button btnCss="danger" name="取消" id="cancel" icon="fa-eraser"
					clickFun="Xgt_wy_topicInfoDlg.close()" /> -->
			</div>
		</div>
	</div>
</div>
	<script src="${ctxPath}/static/addons/zcgl/topic/js/xgt_wy_topic.js"></script>
	@}