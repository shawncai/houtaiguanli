<script>


    <!-- 加载入库单编号 -->
    $(function () {
        var ajax = new $ax(Feng.ctxPath + "/xgt_goods_store_bill/getInStoreNo", function (data) {
            document.getElementById("inBillNo").innerHTML=data;
        }, function (data) {
            Feng.error("加载失败!" + data.responseJSON.message + "!");
        });
        ajax.start();
    })

</script>
