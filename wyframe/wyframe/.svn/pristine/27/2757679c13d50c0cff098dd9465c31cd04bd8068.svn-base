<body class="gray-bg">
<div class="wrapper wrapper-content animated fadeInRight" >
    <div class="row">
        <div class="col-sm-12">

            <div class="ibox float-e-margins" style="margin-bottom: 77px">

                <div class="ibox-title">
                    <div class="text-nowrap" style="float:left;margin-top:16px;margin-left: 2px"><em style="font-size: 20px;font-style: normal;font-weight: bold;color: #2f4050bd">N</em><o style="color:#2f4050bd;font-weight:bold">O.</o><span id="inBillNo" style="color:#00000091">QTKRJ</span></div>
                    <h3 class="name" style="width:106px;float: left;margin-left: 32%;font-weight: bold">入库单</h3>
                    <div class="anniu " style="padding-left: 88%;">
                        <button type="button" class="btn btn-info  btn-xs" id="seach">查询</button>
                        <div class="btn-group">
                            <button data-toggle="dropdown" class="btn btn-info btn-xs dropdown-toggle">批量
                                <span class="caret"></span>
                            </button>
                            <ul class="dropdown-menu" style="min-width:100%;">
                                <li>
                                    <a >导入</a>
                                </li>
                                <li>
                                    <a  class="font-bold">导出</a>
                                </li>
                            </ul>
                        </div>
                        <button type="button" class="btn btn-white  btn-xs" id="print">打印</button>

                    </div>
                </div>
                <div class="ibox-content">
                    <div class="col-sm-3" >

                        <div class="input-group" style="margin-left: -17px">
                            <div class="input-group-btn">
                                <button data-toggle="dropdown" class="btn btn-white dropdown-toggle" type="button">入库仓库
                                </button>
                            </div>
                            <input type="text1" class="form-control case" id="condition1" placeholder="" style="border-radius: 3px">
                            <input type="hidden" id="condition1_hidden" value="">
                            <a style="pointer-events: auto;position: absolute;left: 217px;z-index: 10000;top:8px" id="case_close_span"><img src="${ctxPath}/static/img/delete.png"> </a>
                            <a style="pointer-events: auto;position: absolute;left: 229px;z-index: 10000;top:8px" id="case_add_span"><img src="${ctxPath}/static/img/add.png"> </a>
                            <a style="pointer-events: auto;position: absolute;left: 238px;z-index: 10000;top:9px" id="case_seach_span"><img src="${ctxPath}/static/img/seach.png"> </a>
                        </div>
                    </div>
                    <div class="col-sm-3" style="margin-left: -17px">
                        <div class="input-group">
                            <div class="input-group-btn">
                                <button data-toggle="dropdown" class="btn btn-white dropdown-toggle" type="button">供货商
                                </button>
                            </div>
                            <input type="text" class="form-control addperson gonghuo " id="condition"  value="" style="border-radius: 3px">
                            <input type="hidden" id="condition_hidden" value="">
                            <a style="pointer-events: auto;position: absolute;left: 199px;z-index: 10000;top:8px" id="close_span"><img src="${ctxPath}/static/img/delete.png"></a>
                            <a style="pointer-events: auto;position: absolute;left: 211px;z-index: 10000;top:8px" id="add_span"><img src="${ctxPath}/static/img/add.png"></a>
                            <a style="pointer-events: auto;position: absolute;left: 219px;z-index: 10000;top:9px" id="seach_span"><img src="${ctxPath}/static/img/seach.png"></a>
                        </div>
                    </div>

                    <div class="col-sm-3" style="margin-left: -17px">
                        <div class="input-group">
                            <div class="input-group-btn">
                                <button data-toggle="dropdown" class="btn btn-white dropdown-toggle" type="button">经手人
                                </button>
                            </div>
                            <input type="text2" class="form-control staff" id="condition2" placeholder="" style="border-radius: 3px">
                            <input type="hidden" id="condition2_hidden" value="">
                            <a style="pointer-events: auto;position: absolute;left: 199px;z-index: 10000;top:8px" id="staff_close_span"><img src="${ctxPath}/static/img/delete.png"></a>
                            <a style="pointer-events: auto;position: absolute;left: 211px;z-index: 10000;top:8px" id="staff_add_span"><img src="${ctxPath}/static/img/add.png"></a>
                            <a style="pointer-events: auto;position: absolute;left: 219px;z-index: 10000;top:9px" id="staff_seach_span"><img src="${ctxPath}/static/img/seach.png"></a>
                        </div>
                    </div>
                    <div class="col-sm-3" style="margin-left: -17px">
                        <div class="input-group">
                            <div class="input-group-btn">
                                <button data-toggle="dropdown" class="btn btn-white dropdown-toggle" type="button">日期
                                </button>
                            </div>
                            <input id="hello" class="laydate-icon form-control layer-date" style="border-radius: 3px">
                        </div>
                    </div>
                    <div class="hidden-xs demoTable" id="demoTable" role="group" style="margin-top: 49px;margin-bottom: -11px">
                        <button type="button" class="btn btn-success"  id="add_some">
                            <i class="glyphicon glyphicon-plus" aria-hidden="true"></i>
                            添加
                        </button>
                        <button type="button" class="btn btn-outline btn-default" id="delete_all">
                            <i class="glyphicon glyphicon-trash" aria-hidden="true"></i>
                            删除
                        </button>

                    </div>
                    <table class="layui-table" id="test" lay-filter="demo"></table>
                </div>

            </div>
            <!-- End Example Toolbar -->
        </div>
    </div>
</div>
<div class="navbar-fixed-bottom ibox_footer " style="padding-left: 51px;background: #fff;">
    <div class="row" style="margin-left: -25px;margin-bottom: 10px">

        <div class="col-sm-12" style="width: 98%">
            <textarea cols="101"  id="remark" style="height:27px;width: 98%;" placeholder="备注"></textarea>
        </div>

    </div>
    <div class="row" style="width: 90%;margin-left: -26px;margin-bottom: 11px">
        <div class="col-sm-4">
            <h3>制单人：<span>${user}</span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;打印次数：${print}</h3>

        </div>



    </div>
    <div class="row" style="margin-left: -26px;">
        <div class="col-sm-4"><h3>制单时间：${timer}</h3></div>
    </div>
    <div class="col-sm-4 text-center"  >
        <div class="col-sm-5 " >
            <h3  class="text-left" style="margin-top: 10px;">总计:&nbsp;&nbsp;&nbsp;<span id="totalAmount" style="color: red;font-size: 30px">0.00</span></h3>

        </div>
        <button type="button" class="btn btn-success  btn-lg" id="baocun">保存</button>
        <button type="button" class="btn btn-primary  btn-lg" id="tijiao">提交</button>
    </div>
</div>
