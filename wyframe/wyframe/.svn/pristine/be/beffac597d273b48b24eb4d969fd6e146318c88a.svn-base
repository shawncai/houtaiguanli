@layout("/common/_container.html"){
<div class="ibox float-e-margins">
    <div class="ibox-content">
        <div class="form-horizontal">

           <input type="hidden" id="image_file_id" value="${xyd_image_file.image_file_id}">

           <table class="table table-striped table-bordered table-hover" style="font-size:16px;">
                <tbody>
                 <tr>
                <td class="td_title" style="text-align:center ">名称</td>
            	<td class="td_content" style="text-align:center "><input id="image_file_nm" value="${xyd_image_file.image_file_nm}" readonly="readonly" class="form-control"/></td>
           		<td class="td_title" style="text-align:center ">原始图url</td>
                <td class="td_content" style="text-align:center "><input id="image_file_src_url" value="${xyd_image_file.image_file_src_url}" readonly="readonly" class="form-control"/></td>
                </tr>
                 <tr>
                <td class="td_title" style="text-align:center ">缩略图url</td>
            	<td class="td_content" style="text-align:center "><input id="image_file_url" value="${xyd_image_file.image_file_url}" readonly="readonly" class="form-control"/></td>
           		<td class="td_title" style="text-align:center ">文件url</td>
                <td class="td_content" style="text-align:center "><input id="file_url" value="${xyd_image_file.file_url}" readonly="readonly" class="form-control"/></td>
                </tr>
                 <tr>
                <td class="td_title" style="text-align:center ">文件类型</td>
            	<td class="td_content" style="text-align:center "><input id="img_file_cls" value="${xyd_image_file.img_file_cls}" readonly="readonly" class="form-control"/></td>
           		<td class="td_title" style="text-align:center ">长度</td>
                <td class="td_content" style="text-align:center "><input id="img_long" value="${xyd_image_file.img_long}" readonly="readonly" class="form-control"/></td>
                </tr>
                 <tr>
                <td class="td_title" style="text-align:center ">宽带</td>
            	<td class="td_content" style="text-align:center "><input id="img_wid" value="${xyd_image_file.img_wid}" readonly="readonly" class="form-control"/></td>
           		<td class="td_title" style="text-align:center ">创建时间</td>
                <td class="td_content" style="text-align:center "><input id="cre_dt" value="${xyd_image_file.cre_dt}" readonly="readonly" class="form-control"/></td>
                </tr>
                 <tr>
                <td class="td_title" style="text-align:center ">使用状态</td>
            	<td class="td_content" style="text-align:center "><input id="st_id" value="${xyd_image_file.st_id}" readonly="readonly" class="form-control"/></td>
           		<td class="td_title" style="text-align:center ">操作人</td>
                <td class="td_content" style="text-align:center "><input id="oper_user" value="${xyd_image_file.oper_user}" readonly="readonly" class="form-control"/></td>
                </tr>
                 </tbody>
                </table>
            <div class="row btn-group-m-t">
                <div class="col-sm-10">
                    <#button btnCss="info" name="确定" id="ensure" icon="fa-check" clickFun="Xyd_image_fileInfoDlg.close()"/>
                    <#button btnCss="danger" name="关闭" id="cancel" icon="fa-eraser" clickFun="Xyd_image_fileInfoDlg.close()"/>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="${ctxPath}/static/addons/zcgl/image/js/xyd_image_file_info.js"></script>
<script>

	/**
	 *参数表页面table
	 */
	var Xyd_image_file_detail = {
	    id: "detailTable",	//表格id
	    seItem: null,		//选中的条目
	    table: null,
	    layerIndex: -1
	};

	/**
	 * 初始化表格的列
	 */
	Xyd_image_file_detail.initColumn = function () {
	    return [
	        {field: 'selectItem', checkbox: true}
	    ];
	    
	};
	var id = $("#image_file_id").val();
	var defaultColunmss = Xyd_image_file_detail.initColumn();
    var tabless = new BSTable(Xyd_image_file_detail.id, "/xyd_image_file/detail/"+id, defaultColunmss);
    tabless.setPaginationType("client");
  	Xyd_image_file_detail.table = tabless.init();
</script>
@}
