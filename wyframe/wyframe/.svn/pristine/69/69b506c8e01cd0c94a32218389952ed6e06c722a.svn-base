@layout("/common/_container.html"){
<div class="ibox float-e-margins">
    <div class="ibox-content">
        <form class="form-horizontal" id="signupForm">

             <input type="hidden" id="bill_id" value="${bs_bill.bill_id}">
				
			 <div class="row">
                                             
                                             
                                             
                                             
                                             <#inputCol2 id='access_mem' name='收货人' value='${bs_bill.access_mem}' />
                                             <#inputCol2 id='access_addr_desc' name='收货人地址' value='${bs_bill.access_addr_desc}' />
                                             <#inputCol2 id='access_phone' name='收货人电话' value='${bs_bill.access_phone}' />


                                            <input type="hidden" value="${bs_bill.prov_id}" id="sheng">
                                             <#select2  id='prov_id' name="省市" value='${bs_bill.prov_id}' />
                 <input type="hidden" value="${bs_bill.city_id}" id="cheng">
                                             <#select2  id='city_id' name="城市" value='${bs_bill.city_id}' />
                 <input type="hidden" value="${bs_bill.area_id}" id="qu">
                                             <#select2  id='area_id' name="区县" value='${bs_bill.area_id}' />
                                             

                                             <#inputCol2 id='user_remark' name='用户备注' value='${bs_bill.user_remark}' />
                                             <#inputCol2 id='remarks' name='系统备注' value='${bs_bill.remarks}' />
                                             
                                             
                                             
                                             
                                             
                                             <#inputCol2 id='card_no' name='水卡卡号' value='${bs_bill.card_no}' />
                                             <#inputCol2 id='src_bill_no' name='原始单号' value='${bs_bill.src_bill_no}' />
                                             
                                             
                                             
                           </div>
                            <div class="hidden-xs demoTable" id="demoTable" role="group" style="margin-top: 56px;">
                                           <button type="button" class="btn btn-outline btn-default"  id="add_some2">
                                               <i class="glyphicon glyphicon-plus" aria-hidden="true"></i>
                                           </button>
                                           <button type="button" class="btn btn-outline btn-default" id="delete_all2">
                                               <i class="glyphicon glyphicon-trash" aria-hidden="true"></i>
                                           </button>
                                       </div>
                                       <#layUITable id="Bs_billParamTable3"/>
            <div class="row btn-group-m-t text-center">
                <div class="col-sm-10">
                    <#submit btnCss="info" iName="提交" id="ensure" icon="fa-check" clickFun="validate();Bs_billInfoDlg.editSubmit()"/>
                    <#button btnCss="danger" name="取消" id="cancel" icon="fa-eraser" clickFun="Bs_billInfoDlg.close()"/>
                </div>
            </div>
        </form>
    </div>
</div>
<script src="${ctxPath}/static/addons/zsh/bs_bill/js/bs_bill_info.js"></script>
<style>
    .layui-table-cell{
        height:auto;
        line-height: 41px;
    }</style>
@}
